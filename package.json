{
  "name": "yt-dlp-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch src/server/index.ts\"",
    "build": "node scripts/build-server.mjs && vite build",
    "build:server": "node scripts/build-server.mjs",
    "build:client": "vite build",
    "start": "node dist/server.cjs",
    "smoke": "node scripts/smoke.mjs",
    "test:security": "node scripts/test-security.mjs",
    "test": "vitest run --dir test",
    "test:watch": "vitest --dir test",
    "build:smoke": "npm run build && npm run smoke",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.19.2",
    "express-rate-limit": "7.4.0",
    "framer-motion": "11.3.30",
    "helmet": "7.1.0",
    "lowdb": "6.1.1",
    "lucide-react": "0.468.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.23.8",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.12.12",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.20",
    "concurrently": "8.2.2",
    "esbuild": "0.23.0",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.10",
    "tsx": "4.18.0",
    "typescript": "5.6.3",
    "vite": "5.4.2",
    "vitest": "2.0.5"
  }
}